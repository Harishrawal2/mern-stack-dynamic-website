<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <link rel="stylesheet" href="/static/css/slider.css">
  {{!-- bootstrap links --}}
  <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous" />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.9.0/css/all.min.css"
    integrity="sha512-q3eWabyZPc1XTCmF+8/LuE1ozpg5xxn7iO89yfSOd5/oKvyqLngoNGsx8jq92Y8eXJ/IRxQbEC+FGSYxtk2oiw=="
    crossorigin="anonymous" referrerpolicy="no-referrer" />
  <title>Coders College</title>

</head>

<body>
  {{>navbar data=Details}}

  {{>slider slides=Slides }}

  {{!-- banner section --}}
  <section class="banner1_section py-5 background1">
    <div class="container text-center">
      <div class="row">
        {{#each UpTitles}}
        <div class="col-md-12">
          <h1>{{this.titleUp}}</h1>
          <p>{{this.subTUp}}</p>

         <a href="{{this.linkUp}}" target="_blank"><button class="btn btn-outline-primary">{{this.linkTextUp}}</button> </a> 
        </div>
        {{/each}}
      </div>
    </div>
  </section>

  {{!-- services section --}}

  <section id="services_section" class="py-4">

    <div class="container">
      <h1 class="text-center text-secondary">Services that we provide</h1>
      <div class="row mt-5">
        {{!-- first service column --}}
        {{#each services}}
        <div class="col-md-4 py-2">
          <div class="card">
            <div class="card-body">
              <div class="container text-center">
                <i class="{{this.icon}} service_icon"></i>
              </div>
              <h3 class="card-title">{{this.title}}</h3>
              <p>{{this.description}} </p>
              <button class="btn btn-outline-primary pl-5">
                <a href="{{this.linkText}}" class="linkText" target="_blank">{{this.link}}</a>
              </button>
              <button class="btn btn-outline-info pl-5">
                <a href="{{this.supportLink}}" target="_blank">Support</a>
              </button>
            </div>
          </div>
        </div>


        {{/each}}



      </div>



    </div>
  </section>

  {{!-- bannner 2 section --}}
  {{#each Banners}}
  <section class="banner2_section py-5">
    <div class="container">
      <div class="row">
        <div class="col-md-6 text-center">
          {{!-- left side --}}
          <h3>{{this.titleRight}}</h3>
          <p>{{this.subTRight}}
          </p>
          <button class="btn btn-outline-primary"><a href="{{this.linkTextRight}}"> {{this.linkRight}}</a></button>
        </div>
        <div class="col-md-6 text-center py-4">
          {{!-- right side --}}
          <img src="{{this.imgRight}}" style="max-height: 200px;" alt="">
        </div>
      </div>
    </div>
  </section>

  {{!-- banner 3 section --}}
  <section class="banner2_section py-5">
    <div class="container">
      <div class="row">
        <div class="col-md-6 text-center py-4">
          {{!-- left side --}}
          <img src="{{this.imgLeft}}" style="max-height: 200px;" alt="">
        </div>
        <div class="col-md-6 text-center">
          {{!-- right side --}}
          <h3>{{this.titleLeft}}</h3>
          <p>{{this.subTLeft}}
          </p>
          <button class="btn btn-outline-primary"> <a href="{{this.linkTextLeft}}">
            {{this.linkLeft}}
          </a> </button>
        </div>
      </div>
    </div>
  </section>
        {{/each}}


  {{!-- Contact us section --}}

  <section id="contact_section" class="py-5 background1">
    <div class="container">
      <h3 class="text-center text-secondary">
        Contact Us
      </h3>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <form action="/process-contact-form" method="post">
            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label">Email address</label>
              <input type="email" name="email" class="form-control" id="exampleInputEmail1"
                aria-describedby="emailHelp">
              <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
            </div>
            <div class="mb-3">
              <label for="contactNumberField" class="form-label">Contact Number</label>
              <input type="contact" name="phone" class="form-control" id="contactNumberField">
            </div>

            <div class="form-floating mt-2 mb-3">
              <span>Your Query</span>
              <textarea name="query" class="form-control" placeholder="Leave a query here" id="queryField"
                style="height: 200px"></textarea>
              <label for="queryField"> Your query</label>
            </div>
            <div class="container text-center">
              <button type="submit" class="btn btn-primary">Submit</button>

            </div>
          </form>
        </div>
      </div>
    </div>
  </section>

  {{!-- About and Testimonial section --}}
  <section class="about_section py-5" id="about_section">
    <div class="container text-center">
      <div class="row">
        <div class="col-md-12">
          <h3>About Me</h3>
